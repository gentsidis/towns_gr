<p id="notice"><%= notice %></p>

<h1>Regional Unities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Region</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regional_unities.each do |regional_unity| %>
      <tr>
        <td><%= regional_unity.name %></td>
        <td><%= regional_unity.region %></td>
        <td><%= link_to 'Show', regional_unity %></td>
        <td><%= link_to 'Edit', edit_regional_unity_path(regional_unity) %></td>
        <td><%= link_to 'Destroy', regional_unity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @regional_unities %>
<br>

<%= link_to 'New Regional Unity', new_regional_unity_path %>
