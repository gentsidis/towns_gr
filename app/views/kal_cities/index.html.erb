<p id="notice"><%= notice %></p>

<h1>Kal Cities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Regional unity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kal_cities.each do |kal_city| %>
      <tr>
        <td><%= kal_city.name %></td>
        <td><%= kal_city.title %></td>
        <td><%= kal_city.regional_unity %></td>
        <td><%= link_to 'Show', kal_city %></td>
        <td><%= link_to 'Edit', edit_kal_city_path(kal_city) %></td>
        <td><%= link_to 'Destroy', kal_city, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @kal_cities %>
<br>

<%= link_to 'New Kal City', new_kal_city_path %>
